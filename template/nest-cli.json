{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      {
        "include": "../src/pages/**/*.html",
        "outDir": "dist/pages"
      }
    ],
    "watchAssets": true
  }
}
